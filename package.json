    "rollup": "^2.60.2",
    "rollup-plugin-esbuild": "^4.7.2",
    "yargs": "^17.2.1",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-less": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/yargs": "^17.0.4",
    "pkg": "^5.4.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "lib"
  ],
